{
	"name": "react",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"build:dev": "tsc && vite build --mode development",
		"build:test": "tsc && vite build --mode test",
		"build:pro": "tsc && vite build --mode production",
		"preview": "vite preview",
		"lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:lint-staged": "lint-staged",
		"prepare": "husky install",
		"release": "standard-version",
		"commit": "git pull && git add -A && git-cz && git push",
		"deploy": "pnpm build:pro && cross-env NODE_ENV=production node ./deploy"
	},
	"dependencies": {
		"@ant-design/icons": "5.1.4",
		"antd": "^5.6.4",
		"axios": "^0.27.2",
		"driver.js": "^0.9.8",
		"echarts": "^5.4.2",
		"echarts-liquidfill": "^3.1.0",
		"i18next": "^21.10.0",
		"immer": "^9.0.21",
		"js-md5": "^0.7.3",
		"moment": "^2.29.4",
		"nprogress": "^0.2.0",
		"qs": "^6.11.2",
		"react": "^18.2.0",
		"react-activation": "^0.11.2",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.10",
		"react-i18next": "^11.18.6",
		"react-redux": "^8.1.1",
		"react-router-dom": "^6.14.1",
		"react-transition-group": "^4.4.5",
		"redux": "^4.2.1",
		"redux-persist": "^6.0.0",
		"redux-promise": "^0.6.0",
		"redux-thunk": "^2.4.2",
		"screenfull": "^6.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@types/node": "^17.0.45",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-promise": "^0.5.29",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"@vitejs/plugin-react": "^1.3.2",
		"autoprefixer": "^10.4.14",
		"chalk": "3.0.0",
		"commitizen": "^4.3.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.6.1",
		"dotenv": "^16.3.1",
		"dotenv-expand": "^10.0.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"less": "^4.1.3",
		"lint-staged": "^13.2.3",
		"ora": "4.0.3",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"rollup-plugin-visualizer": "^5.9.2",
		"scp2": "^0.5.0",
		"standard-version": "^9.5.0",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^3.1.0",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-less": "^1.0.7",
		"terser": "^5.18.2",
		"typescript": "^4.9.5",
		"vite": "^4.3.9",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	}
}
